import daf.print;

#macro VERSION 2.4

def main:=(argc:int, argv:&&char) -> int {
	printf("Hello");
	let mut a:=4;
	let b:=&mut a;
	a++;
	b[0]++;
	@b++; //At b, add one
	
	#ifmacro VERSION
		printf("The version is #VERSION");
	#endif

	refTo(mut a, b);
	smartRefTo(a, b);
}

def refTo:=(&mut a:int, b:&mut int) -> boolean {
	let copy:=a;
	@b++;
	return a!=copy;
}

def smartRefTo:=(&a:int, b:&int) -> boolean {
	return &a==b;
}