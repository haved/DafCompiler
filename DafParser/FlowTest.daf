
#macro ToTrueOrFalse<val> #if val #then<>true#else<>false#endif
#macro Not<val> #if #val #then 0 #else 1 #endif

#macro VERSION 255

#if #Not<#(#VERSION -255 +#)> #then
    printf("The version is 255!");
#endif

#macro TestCase<> $Some Else 5 Last$

#(#TestCase#) //Last
#(#TestCase +#) //5Last
#(#TestCase + + +#) //SomeElse5Last
#(5 2 +#) //7